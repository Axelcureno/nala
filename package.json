{
    "name": "janus",
    "version": "1.0.0",
    "description": "UI Automation framework for end to end testing of Milo/Helix block libraries",
    "main": "run.js",
    "scripts": {
        "start": "node run.js",
        "clean": "node tools/clean",
        "ci": "yarn install --frozen-lockfile",
        "test": "jest",
        "install-jsdoc": "handpick --target=jsdocDependencies --manager=yarn",
        "install-cld": "handpick --target=dependencies --target=cldDependencies --manager=yarn",
        "install-analytics": "handpick --target=dependencies --target=analyticsDependencies --manager=yarn",
        "install-imageComparison": "handpick --target=dependencies --target=imageComparison --manager=yarn",
        "install-test": "handpick --target=dependencies --target=testDependencies --manager=yarn",
        "install-all": "handpick --target=dependencies --target=cldDependencies --target=analyticsDependencies --target=jsdocDependencies --target=imageComparisonDependencies --target=fedsDependencies --manager=yarn",
        "jsdoc": "jsdoc -r -c ./jsdoc.config.json",
        "cuke_stats": "node tools/get_cuke_stats",
        "timestamp": "node tools/timestamp",
        "upgradeBrowser": "node tools/upgrade_browser",
        "syncJira": "node tools/sync_jira",
        "checkCukeScenarios": "node tools/check_cuke_scenarios",
        "screenshotCompare": "node tools/screenshot/compare",
        "doctor": "node tools/doctor",
        "missing_steps": "node tools/get_missing_steps",
        "pretest": "rimraf allure-results && rimraf allure-report",
        "report-generate": "allure generate --clean allure-results",
        "report-serve": "allure serve allure-results",
        "report-open": "allure open allure-report"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:adobecom/janus.git"
    },
    "author": "Adobe Milo Engineering & Web Platform",
    "private": false,
    "devDependencies": {},
    "dependencies": {
        "@babel/core": "^7.17.7",
        "@babel/node": "^7.16.8",
        "@babel/plugin-proposal-export-default-from": "^7.16.7",
        "@babel/preset-env": "^7.16.11",
        "@babel/register": "^7.17.7",
        "@cucumber/message-streams": "^4.0.0",
        "@cucumber/messages": "^19.0.0",
        "@cucumber/gherkin-streams": "^5.0.1",
        "@cucumber/gherkin": "^24.0.0",
        "@wdio/cli": "~7.20.1",
        "@wdio/cucumber-framework": "7.20.0",
        "@wdio/devtools-service": "~7.20.1",
        "@wdio/local-runner": "~7.20.1",
        "@wdio/mocha-framework": "~7.20.0",
        "@wdio/selenium-standalone-service": "~7.20.1",
        "@wdio/spec-reporter": "~7.20.0",
        "axe-core": "^4.3.5",
        "axios": "^0.24.0",
        "expect": "^27.3.1",
        "expect-webdriverio": "^3.1.4",
        "firefox-profile": "^4.2.2",
        "glob": "^7.2.0",
        "handpick": "^3.2.1",
        "he": "~1.2.0",
        "imapflow": "^1.0.76",
        "jira-client": "^7.1.0",
        "js-yaml": "^4.1.0",
        "jsdom": "^18.0.1",
        "mime-types": "^2.1.34",
        "moment": "^2.29.1",
        "polytype": "^0.14.0",
        "puppeteer-core": "^13.1.3",
        "rgb2hex": "^0.2.3",
        "typescript": "^4.6.2",
        "url-join": "^4.0.1",
        "wdio-chromedriver-service": "^7.3.1",
        "wdio-cucumberjs-json-reporter": "^4.4.0",
        "wdio-docker-service": "~3.1.4",
        "webdriverio": "~7.20.0",
        "wgxpath": "^1.2.0",
        "xml-js": "^1.6.11",
        "yargs": "^17.2.1"
    },
    "testDependencies": {
        "jest": "^27.3.1",
        "handpick": "^3.2.1"
    },
    "cldDependencies": {
        "cld": "~2.6.0",
        "handpick": "^3.2.1"
    },
    "analyticsDependencies": {
        "anyproxy": "^4.1.2",
        "handpick": "^3.2.1"
    },
    "jsdocDependencies": {
        "yargs": "^17.2.1",
        "glob": "^7.2.0",
        "jsdoc": "^3.6.6",
        "js-yaml": "^4.1.0",
        "escodegen": "^2.0.0",
        "esprima": "^4.0.1",
        "mime-types": "~2.1.26",
        "handpick": "^3.2.1"
    },
    "imageComparisonDependencies": {
        "wdio-image-comparison-service": "^3.0.0"
    },
    "fedsDependencies": {
        "chai": "^4.3.4",
        "@wdio/allure-reporter": "^7.16.6"
    }
}